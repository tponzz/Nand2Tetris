/**
 * And gate:
 * if (a and b) out = 1, else out = 0 
 */
CHIP Neg {
    IN in[16];
    OUT out;
    
    PARTS:
    //// Replace this comment with your code.
    And(a=in[15], b=in[15], out=out);
}