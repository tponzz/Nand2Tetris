/**
 * 8-way Or gate: 
 * out = in[0] Or in[1] Or ... Or in[7]
 */
CHIP Or16Way {
    IN in[16];
    OUT out;

    PARTS:
    Or8Way(in=in[0..7], out=ho);
    Or8Way(in=in[8..15], out=lo);
    Or(a=ho, b=lo, out=out);
}